<%= form_for(@post) do |f|%>
<label>title:</label>
<%= f.text_field(:title) %>
<label>Content:</label>
<%= f.text_field(:content) %>
<label>Likes:</label>
<%= f.number_field(:likes) %>
<label>Blogger:</label>
<%= f.collection_select(:blogger_id, Blogger.all, :id, :name) %>
<label>Destination:</label>
<%= f.collection_select(:destination_id, Destination.all, :id, :name) %>

<%= f.submit("Submit")%>
<%end%>
